{
  "extends": [
    "google",
    "plugin:react/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "env": {
    "browser": true,
    "amd": true,
    "es2020": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["babel", "react", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "require-jsdoc": "off",
    "no-eval": "error",
    "no-console": "off",
    "no-invalid-this": "off",
    "babel/no-invalid-this": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
