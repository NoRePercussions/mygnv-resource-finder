{
    "name": "mygnv-resource-finder-api",
    "private": true,
    "version": "1.0.0",
    "description": "MyGNV Resource Finder API",
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build": "./node_modules/.bin/nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "dev": "nest start --watch",
        "debug": "nest start --debug --watch",
        "prod": "NODE_ENV=production node ./dist/main",
        "lint": "eslint \"{src,apps,libs,test,util}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test,util}/**/*.ts\" --fix",
        "docs:gen": "compodoc -p tsconfig.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^7.4.4",
        "@nestjs/core": "^7.4.4",
        "@nestjs/jwt": "^7.1.0",
        "@nestjs/mongoose": "^7.0.2",
        "@nestjs/passport": "^7.1.0",
        "@nestjs/platform-express": "^7.1.1",
        "@nestjs/serve-static": "^2.1.3",
        "@nestjs/swagger": "^4.6.0",
        "@typegoose/typegoose": "^7.3.4",
        "bcrypt": "^5.0.0",
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "mongoose": "^5.10.2",
        "nestjs-typegoose": "^7.1.34",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.5",
        "swagger-ui-express": "^4.1.4"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.11",
        "@nestjs/cli": "^7.2.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.1.1",
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.6",
        "@types/jest": "^25.2.3",
        "@types/mongoose": "^5.7.36",
        "@types/ms": "^0.7.31",
        "@types/node": "^14.0.6",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.9",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "axios": "^0.20.0",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "jest": "^26.0.1",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "supertest": "^4.0.2",
        "ts-jest": "^26.1.0",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
